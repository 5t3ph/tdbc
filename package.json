{
  "name": "thinkdobecreate",
  "version": "1.0.0",
  "description": "Web home of Stephanie Eckles - @5t3ph",
  "main": "index.html",
  "scripts": {
    "build:sass": "sass src/sass:src/css",
    "build:eleventy": "eleventy",
    "build:css": "postcss public/css/*.css -u autoprefixer cssnano -r -m",
    "watch:sass": "sass --watch src/sass:src/css",
    "watch:eleventy": "eleventy --serve",
    "start": "ELEVENTY_ENV=dev npm-run-all build:sass --parallel watch:*",
    "build": "ELEVENTY_ENV=prod npm-run-all build:* screenshot",
    "lint": "stylelint 'src/sass/**/*.scss' 'src/sass/**/**/*.scss' 'src/sass/**/**/**/*.scss'",
    "lint:fix": "stylelint --fix 'src/sass/**/*.scss' 'src/sass/**/**/*.scss' 'src/sass/**/**/**/*.scss'",
    "screenshot": "node functions/images.js",
    "bump": "npm --no-git-tag-version version"
  },
  "keywords": [
    "11ty",
    "eleventy",
    "portfolio"
  ],
  "author": "5t3ph",
  "license": "CC BY-NC 4.0",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "emoji-regex": "^9.0.0",
    "luxon": "^1.23.0",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^6.0.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.0.0",
    "puppeteer": "5.3.1",
    "rss-parser": "^3.9.0",
    "sass": "^1.26.11",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-selector-bem-pattern": "^2.1.0",
    "terser": "^5.0.0"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "chrome-aws-lambda": "5.3.1",
    "puppeteer-core": "5.3.1"
  }
}

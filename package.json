{
  "name": "thinkdobecreate",
  "version": "1.0.0",
  "description": "Web home of Stephanie Eckles - @5t3ph",
  "main": "index.html",
  "scripts": {
    "watch:sass": "sass  --no-source-map --watch src/sass:public/css",
    "watch:eleventy": "eleventy --serve",
    "build:sass": "sass  --no-source-map src/sass:public/css",
    "build:eleventy": "eleventy",
    "postbuild": "postcss public/css/*.css -u autoprefixer cssnano -r --no-map",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:sass build:eleventy social-images",
    "lint": "stylelint 'src/sass/**/*.scss' 'src/sass/**/**/*.scss' 'src/sass/**/**/**/*.scss'",
    "lint:fix": "stylelint --fix 'src/sass/**/*.scss' 'src/sass/**/**/*.scss' 'src/sass/**/**/**/*.scss'",
    "social-images": "eleventy-social-images --siteName 'ThinkDoBeCreate' --outputDir public --dataFile src/_generate/pages.json",
    "bump": "npm --no-git-tag-version version"
  },
  "keywords": [
    "11ty",
    "eleventy",
    "portfolio"
  ],
  "author": "5t3ph",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@11tyrocks/eleventy-plugin-social-images": "^0.1.3",
    "autoprefixer": "^10.2.4",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "rss-parser": "^3.12.0",
    "sass": "^1.32.8",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "terser": "^5.6.0"
  },
  "browserslist": [
    "last 2 versions"
  ]
}

---
layout: post
pagination:
  data: prismic.post
  size: 1
  alias: post
  addAllPagesToCollections: true
tags: prismic
permalink:
  build: /posts/{{post.uid}}/
  preview: /preview/posts/:uid/
eleventyComputed:
  title: "{% asText post.data.title %}"
  postdate: "{{ post.data.publish_date }}"
  description: "{% asText post.data.summary %}"
---

{% asHTML post.data.content %}
